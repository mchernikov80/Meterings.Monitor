@model MonitoringWeb.Models.MeteringDevicesHistoryModel
@{
    ViewBag.Title = "История замены счетчиков";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <h2>История замены счетчиков</h2>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h2 class="panel-title">@Html.LabelFor(m => m.MeteringPoint): @Html.DisplayFor(m => m.MeteringPoint)</h2>
            </div>
            <div class="panel-body">
                @Html.Partial("_MeteringDevicesInfoList", @Model.MeteringDevicesHistory)
            </div>
        </div>
    </div>
</div>

